Components {
  Main "test.py";
}

Messages {
  M(str, str);
}

Init {
  spawn(Main);
}

Exchange(c) {
  Main {
    M(a, b) {
      send(c, M("1", "a"));
      send(c, M("2", "b"));
      send(c, M("3", "c"));
    }
  }
}

Properties {
  Any =
    Match {
      .*
    };

  Any2 =
    Match {
     .*.*
    };

  AnyOrAny =
    Match {
     .*|.*
    };

  MaybeOneOrAny =
    Match {
      .?|.*
    };

  AnyDisguise =
    Match {
      ([^send M]|[^recv M])*
    };

  NoSendTillRecv =
    Match {
      [^send M]*|.*[recv M][^send M]*
    };

  DonTest =
    Match {
      (.*[recv M])?[^send M]*
    };
}
